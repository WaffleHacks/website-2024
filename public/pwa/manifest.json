{
  "$schema": "http://json.schemastore.org/web-manifest",
  "name": "WaffleHacks 2024",
  "short_name": "WaffleHacks",
  "description": "WaffleHacks is a 48-hour student led and organized virtual hackathon working to bring technical solutions to your local areas.",
  "scope": "/",
  "start_url": "/",
  "dir": "ltr",
  "display": "standalone",
  "orientation": "portrait-primary",
  "display_override": ["minimal-ui","browser","fullscreen"],
  "background_color": "#EEE",
  "theme_color": "#1565c0",
  "icons": [
    {
            "src": "../assets/svgs/logo.svg",
            "type": "image/svg+xml",
            "sizes": "any",
            "purpose": "any"
        },
        {
            "src": "../pwa/android/android-icon-192x192.png",
            "type": "image/png",
            "sizes": "192x192",
            "purpose": "maskable"
        },
        {
            "src": "../assets/images/logo.png",
            "type": "image/png",
            "sizes": "512x512",
            "purpose": "maskable"
        }
    ],
    "screenshots": [
      {
        "src": "../assets/images/Screenshot1.png",
        "sizes": "[width]x[height]",
        "type": "image/png",
        "form_factor": "wide",
        "label": "WaffleHacks 2024"
      },
      {
        "src": "../assets/images/Screenshot2.png",
        "sizes": "[width]x[height]",
        "type": "image/png",
        "form_factor": "narrow",
        "label": "WaffleHacks 2024"
      }
    ]
}